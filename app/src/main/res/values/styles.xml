        <item name="android:fontFamily">@font/notosanskr_light</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="chat_list">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:dividerHeight">8dp</item>
        <item name="android:divider">#00ff0000</item>
    </style>

    <style name="dialog_title">
        <item name="android:textSize">24dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/notosanskr_medium</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="dialog_content">
        <item name="android:textSize">14dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/notosanskr_regular</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="dialog_no">
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#5C5961</item>
        <item name="fontFamily">@font/notosanskr_regular</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="dialog_yes">
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/notosanskr_regular</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="compose_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
